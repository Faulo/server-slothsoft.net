		"slothsoft/dragonage" : "*",
		"slothsoft/farah" : "*",
		"slothsoft/minecraft" : "*",
		"slothsoft/sse" : "*",
		"slothsoft/talesof" : "*",
		"slothsoft/whatthehell" : "*",
