{
	"name" : "slothsoft/slothsoft.net",
	"type" : "project",
	"description" : "slothsoft.net",
	"license" : "GPL",
	"keywords" : [
		"slothsoft",
		"farah-module",
		"farah-server"
	],
	"autoload" : {
		"files" : [
			"config.php",
			"src/registerModule.php"
		]
	},
	"require" : {
		"php" : "^7.2",
		"slothsoft/amber" : "dev-master",
		"slothsoft/chat" : "dev-master",
		"slothsoft/core" : "dev-master",
		"slothsoft/cron" : "dev-master",
		"slothsoft/dragonage" : "dev-master",
		"slothsoft/farah" : "dev-master",
		"slothsoft/minecraft" : "dev-master",
		"slothsoft/sse" : "dev-master",
		"slothsoft/talesof" : "dev-master",
		"slothsoft/whatthehell" : "dev-master"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^6.5"
	},
	"autoload-dev" : {
		"classmap" : [
			"tests/"
		]
	},
	"scripts" : {
		"post-autoload-dump" : "Slothsoft\\Core\\ServerEnvironment::cleanCacheDirectory"
	},
	"classmap-authoritative" : true,
	"minimum-stability" : "dev",
	"prefer-stable" : true
}