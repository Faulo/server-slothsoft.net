{
	"name" : "slothsoft/slothsoft.net",
	"type" : "project",
	"description" : "slothsoft.net",
	"license" : "MIT",
	"keywords" : [
		"slothsoft",
		"farah-module",
		"farah-server"
	],
	"autoload" : {
		"files" : [
			"src/bootstrap.php"
		],
		"psr-4" : {
			"Slothsoft\\Server\\Slothsoft\\" : "src"
		}
	},
	"require" : {
		"slothsoft/amber" : "*",
		"slothsoft/chat" : "*",
		"slothsoft/core" : "^1.14",
		"slothsoft/cron" : "*",
		"slothsoft/farah" : "^1.24",
		"slothsoft/minecraft" : "*",
		"slothsoft/sse" : "^1.0.9",
		"google/apiclient" : "^2.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~9.6.23",
		"slothsoft/farah-testing" : "^1.1.2"
	},
	"autoload-dev" : {
		"classmap" : [
			"tests/"
		],
		"psr-4" : {
			"Slothsoft\\Server\\Slothsoft\\Tests\\" : "tests"
		}
	},
	"scripts" : {
		"farah-asset" : "@php vendor/slothsoft/farah/scripts/farah-asset.php",
		"farah-page" : "@php vendor/slothsoft/farah/scripts/farah-page.php"
	},
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"homepage" : "http://slothsoft.net"
}
