{
    "name": "slothsoft/slothsoft.net",
    "type": "project",
    "description": "slothsoft.net",
    "license": "GPL",
    "keywords": [
        "slothsoft",
        "farah-module",
        "farah-server"
    ],
    "autoload": {
        "files": [
            "config.php",
            "src/registerModule.php"
        ]
    },
    "require": {
        "php": "^7.2",
        "slothsoft/amber": "*",
        "slothsoft/chat": "*",
        "slothsoft/core": "*",
        "slothsoft/cron": "*",
        "slothsoft/dragonage": "*",
        "slothsoft/farah": "*",
        "slothsoft/minecraft": "*",
        "slothsoft/sse": "*",
        "slothsoft/talesof": "*",
        "slothsoft/whatthehell": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-autoload-dump": "Slothsoft\\Core\\ServerEnvironment::cleanCacheDirectory"
    },
    "classmap-authoritative": true
}
